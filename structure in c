//structure basic program to implement marks of student.
#include<stdio.h>
#include<string.h>

struct student
{
  char name[100];
  int roll;
  float cgpa;
};

int main()
{
    struct  student s1={"Adnan",19,8.96};
     printf("student name=%s\n",s1.name);
         printf("student roll no.=%d\n",s1.roll);
         printf("student cgpa=%f",s1.cgpa);
    
       


  return 0;

}


